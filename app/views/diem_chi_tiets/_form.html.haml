= form_for @diem_chi_tiet do |f|
  - if @diem_chi_tiet.errors.any?
    #error_explanation
      %h2= "#{pluralize(@diem_chi_tiet.errors.count, "error")} prohibited this diem_chi_tiet from being saved:"
      %ul
        - @diem_chi_tiet.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :ma_sinh_vien
    = f.text_field :ma_sinh_vien
  .field
    = f.label :ma_lop
    = f.text_field :ma_lop
  .field
    = f.label :ma_mon_hoc
    = f.text_field :ma_mon_hoc
  .field
    = f.label :nam_hoc
    = f.text_field :nam_hoc
  .field
    = f.label :hoc_ky
    = f.number_field :hoc_ky
  .field
    = f.label :diem
    = f.number_field :diem
  .field
    = f.label :loai_diem
    = f.text_field :loai_diem
  .field
    = f.label :lan
    = f.number_field :lan
  .actions
    = f.submit 'Save'
